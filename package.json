{
  "name": "rei-cedar-sketch-tokens",
  "description": "A sketch plugin that prints out cedar tokens as sketch assets",
  "version": "0.1.0",
  "author": "James Bradford",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rei/rei-cedar-sketch-tokens.sketchplugin/issues"
  },
  "homepage": "https://github.com/rei/rei-cedar-sketch-tokens.sketchplugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rei/rei-cedar-sketch-tokens.sketchplugin.git"
  },
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "rei-cedar-sketch-tokens",
    "manifest": "src/manifest.json",
    "main": "rei-cedar-sketch-tokens.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@rei/cdr-tokens": "^1.1.1",
    "@skpm/builder": "^0.7.0",
    "@skpm/fs": "^0.2.5"
  }
}